@Article{Eckert2012,
  Title                    = {Three Waves of Variation Study: The Emergence of Meaning in the Study of Sociolinguistic Variation},
  Author                   = {Penelope Eckert},
  Journal                  = {Annual Review of Anthropology},
  Year                     = {2012},
  Pages                    = {87--100},
  Volume                   = {41},

  _url                     = {https://korpling.german.hu-berlin.de/svn/bibliographie/2012},
  Abstract                 = {The treatment of social meaning in sociolinguistic variation has come
in three waves of analytic practice. The first wave of variation studies
established broad correlations between linguistic variables and the
macrosociological categories of socioeconomic class, sex, class, ethnicity,
and age. The second wave employed ethnographic methods to explore
the local categories and configurations that inhabit, or constitute,
these broader categories. In both waves, variation was seen as marking
social categories. This article sets out a theoretical foundation for the
third wave, arguing that (a) variation constitutes a robust social semiotic
system, potentially expressing the full range of social concerns in
a given community; (b) the meanings of variables are underspecified,
gainingmore specific meanings in the context of styles, and (c) variation
does not simply reflect, but also constructs, social meaning and hence
is a force in social change.},
  File                     = {Eckert2012-Three:.\\2012\\Eckert2012-Three.pdf:PDF},
  Keywords                 = {indexicality, style, enregisterment, social meaning},
  Owner                    = {anke},
  Publisher                = {Annual Reviews},
  Timestamp                = {2018.07.17}
}

